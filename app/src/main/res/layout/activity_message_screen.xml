<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.samad.securemessagingapplayout.MessageScreen"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/SecureMessagingApp"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Secure Messaging App"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="17sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btnNewMessage"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="26dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/btnUnlockMessage1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unlock Message"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/btnNewMessage"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnUnlockMessage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unlock Message"
        android:layout_marginStart="3dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/btnUnlockMessage1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnUnlockMessage1"
        android:layout_marginLeft="3dp" />

    <Button
        android:id="@+id/btnUnlockMessage3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unlock Message"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnUnlockMessage1"
        android:layout_marginTop="84dp"
        app:layout_constraintTop_toBottomOf="@+id/btnUnlockMessage1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnUnlockMessage1" />

    <Button
        android:id="@+id/btnUnlockMessage4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Unlock Message"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnUnlockMessage3"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/btnUnlockMessage3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnUnlockMessage3" />

    <TextView
        android:id="@+id/Contact1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact1"
        android:layout_marginStart="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnUnlockMessage1"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/Contact2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact2"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/btnUnlockMessage4"
        android:layout_marginTop="96dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="95dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnUnlockMessage1"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/Contact3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact3"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/Contact4"
        android:layout_marginTop="59dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="58dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Contact2"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/Contact4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact4"
        android:layout_marginStart="16dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btnUnlockMessage4"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="9dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="46dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btnUnlockMessage1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnNewMessage"
        android:layout_marginLeft="48dp"
        android:layout_marginRight="46dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/checkBox"
        android:layout_marginTop="33dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/checkBox" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/checkBox3"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/checkBox3" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/checkBox4"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/checkBox4" />

    <Button
        android:id="@+id/btnNewMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Message"
        android:layout_marginEnd="4dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="44dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="4dp" />

    <Button
        android:id="@+id/btnDeleteMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delete Message"
        android:layout_marginEnd="4dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="71dp"
        android:layout_marginBottom="57dp"
        app:layout_constraintTop_toTopOf="@+id/checkBox5"
        android:layout_marginRight="4dp" />


</android.support.constraint.ConstraintLayout>
