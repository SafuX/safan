<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.samad.securemessagingapplayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SecureMessagingApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MessageScreen" />
        <activity android:name=".NewMessageScreen" />
        <activity android:name=".SendToContacts" />
        <activity android:name=".PasscodeScreen" />
        <activity android:name=".NewUserScreen" />
        <activity android:name=".ForgetPassword" />
        <activity android:name=".MessageFromContacts"></activity>
    </application>

</manifest>